package SWexpert;
import java.util.*;
public class Swea3376 {
	static Scanner sc = new Scanner(System.in);
	static int C;
	public static void main(String[] args) {
		C = sc.nextInt();
		
		for(int tc=1;tc<=C;tc++) {
			int N = sc.nextInt();
			
			int[] wave = new int[N];
			
			if(N >=5) {
				wave[0] = 1;
				wave[1] = 1;
				wave[2] = 1;
				wave[3] = 2;
				wave[4] = 2;
				
				int result = find(N, wave);
				System.out.println("#" + tc + " " + result);
			}
			
		}

	}
	private static int find(int n, int[] wave) {
		
		if(n<=5) {
			return wave[n-1];
		}else {
			return find(n-1, wave) + find(n-5, wave);
		}
		
		
		
	}

}
