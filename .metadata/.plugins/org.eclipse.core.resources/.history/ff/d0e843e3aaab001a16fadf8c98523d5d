package SWexpert;
//북북서
import java.util.*;


public class Swea8556 {
	static Scanner sc = new Scanner(System.in);
	static int C;
	public static void main(String[] args) {

		C = sc.nextInt();
		
		for(int tc=1;tc<=C;tc++) {
			String st = sc.next();
			
			st = st.replaceAll("west", "0");
			st = st.replaceAll("north","1");
			
			System.out.println(st);
			
		}

	}

}
