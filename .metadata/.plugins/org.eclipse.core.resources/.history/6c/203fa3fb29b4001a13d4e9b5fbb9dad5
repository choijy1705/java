package SWexpert;
import java.util.*;
public class Swea8104 {
	static Scanner sc = new Scanner(System.in);
	static int C;
	static int N;
	static int K;
	public static void main(String[] args) {
		
		C = sc.nextInt();
		
		for(int tc=1;tc<=C;tc++) {
			N = sc.nextInt();
			K = sc.nextInt();
			
			int student = N * K;
			
			int flow = 0;
			
			int[][] classify = new int[K][N];
			
			int n=1;
			int x= 0;
			int y;
			while(x<K) {
				y=0;
				if(flow == 0) {
					for(int i=n;i<n+K;i++) {
						classify[x][y] = i;
					}
					
					flow = 1;
				}else {
					for(int i=n+K-1;i>=n;i--) {
						classify[x][y] = i;
					}
					
					flow = 0;
				}
				x++;
				n = n+K;
			}
			
			int[] sum = new int[K];
			
			for(int i=0;i<K;i++) {
				sum[i] = 0;
				for(int j=0;j<N;j++) {
					sum[i] += classify[i][j];
				}
			}
			
			int max = 0;
			int z = 0;
			String result = "";
			int cnt=0;
			while(cnt < K) {
				for(int i=0;i<K;i++) {
					if(sum[i] > max) {
						max = sum[i];
						z = i;
					}
				}
				sum[z] = 0;
				result += (z+1) + " ";
				cnt++;
			}
			
			System.out.println("#" + tc + " " + result);
			
		}

	}

}
