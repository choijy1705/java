package SWexpert;

import java.util.*;
public class MemoryRestore {
	static Scanner sc = new Scanner(System.in);
	static int C;
	public static void main(String[] args) {
		C = sc.nextInt();
		
		for(int tc=1;tc<=C;tc++) {
			
			String memory = sc.next();
			
			int len = memory.length();
			
			int chk = (1<<len);
			int a = (1<<len)-1;
			int mem = Integer.parseInt(memory);
			int cnt = 0;
			for(int i=0;i<len;i++) {
				if(String.valueOf(chk).charAt(i+1) != memory.charAt(i)) {
					if(memory.charAt(i) == '1') {
						chk += a;
						cnt++;
					}else {
						chk -= a;
						cnt++;
					}
				}
			}
			System.out.println("#" + tc + " " + cnt);
			
		}

	}

}
