package SWexpert;
import java.util.*;

public class Swea6692 {
	static Scanner sc = new Scanner(System.in);
	static int C;
	static int N;
	public static void main(String[] args) {
		C = sc.nextInt();
		
		for(int tc=1;tc<=C;tc++) {
			N = sc.nextInt();
			
			int sum = 0;
			for(int i=0;i<N;i++) {
				int p = sc.nextInt();
				int x = sc.nextInt();
				
				sum += (p*x);
			}
			
			System.out.println("#" + tc + " " + sum);
		}

	}

}
